#  using sorting type and also noraml type

def linear_search(arr, target):
    for i in range(len(arr)):
        if arr[i] == target:
            return i             # element at found
    return-1                    # element not found

"""#n= int(input("enter the numbre of elements:"))
arr=list(map(int, input("enter the elements").split()))
target=int(input("element search:"))
"""
#   (or)
#Noraml type
arr=[1,2,3,4,5]
target=3


result = linear_search(arr, target)
if result!= -1:
    print("element at found:", result)
else:
    print("element not found")