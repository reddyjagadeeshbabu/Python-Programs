def insertion_sort(arr):
    n = len(arr)
    for i in range(1, n):
        key = arr[i]
        j = i - 1

        # Move elements greater than key one step ahead
        while j >= 0 and arr[j] > key:
            arr[j + 1]=arr[j] 
            j -= 1

        arr[j + 1] = key
    return arr


# ---- Main Program ----
n = int(input("Enter number of elements: "))         # number of elements
arr = list(map(int, input("Enter elements: ").split()))  # input array

sorted_arr = insertion_sort(arr)

# Print result
print("Sorted array:",*sorted_arr)