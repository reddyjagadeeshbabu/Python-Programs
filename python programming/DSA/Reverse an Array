
def reverse(ar):
    i, j = 0, len(ar) - 1
    while i < j:
        t=ar[i]
        ar[i] = ar[j]
        ar[j]=t # swap elements
        i += 1
        j -= 1


def main():
    ar = [2, 4, 5, 6, 7, 8, 9]
    print("Original array:", ar)
    reverse(ar)
    print("Reversed array:", ar)


if __name__ == '__main__':
    main()
