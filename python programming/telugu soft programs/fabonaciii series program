# minimal fix recursive using recursion:

def fabonacci(n):
    if n<0:
        return "Input should be a postive integer"
    elif n==1:
        return 0
    elif n==2:
        return 1
    else:
        return fabonacci(n-1)+fabonacci(n-2) 
for i in range(1,11):
    print(fabonacci(i))   