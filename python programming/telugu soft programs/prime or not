# print prime number using loops: given range:

def print_prime(n):
    count = 0
    num = 1               # nuvu anta number este akada numfi print avutundi
    while count < n:
        for i in range(2, num):
            if num%i==0:
                break
        else:
            print(num, end=" ")
            count = count+1
        num +=1
print_prime(10)